<Window x:Class="DW.CodedUI.Demo.FastStartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Application Window Title"
        Height="350"
        Width="525">
    <StackPanel>
        <Button HorizontalAlignment="Left" Content="Show Child Modal" AutomationProperties.AutomationId="ShowModalChild" Click="ShowChildWindowModal" />
        <Button HorizontalAlignment="Left" Content="Show Child Non Modal" AutomationProperties.AutomationId="ShowNonModalChild" Click="ShowChildWindowNonModal" />
    </StackPanel>
</Window>
